{
  "name": "@jsprds/angular-lib-poc",
  "version": "0.0.2",
  "license": "MIT",
  "homepage": "https://github.com/jsprds/angular-lib-poc",
  "author": "Jasper De Smet <jasper.desmet@studiohyperdrive.be>",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --app=school",
    "start:sandbox": "ng serve --app=sandbox",
    "build:lib": "ng-packagr -p libs/jsprds/package.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pages": "ng build --prod --app=school --output-path docs",
    "format": "./node_modules/.bin/nx format write",
    "nx-migrate": "./node_modules/.bin/nx migrate",
    "apps:affected": "./node_modules/.bin/nx affected apps",
    "build:affected": "./node_modules/.bin/nx affected build",
    "e2e:affected": "./node_modules/.bin/nx affected e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "docs:generate": "./node_modules/.bin/compodoc libs -p tsconfig.json -d dist-docs -n 'API documentation' --includes docs",
    "docs:serve": "npm run docs:generate -- --serve --watch"
  },
  "dependencies": {
    "@angular/animations": "5.2.4",
    "@angular/common": "5.2.4",
    "@angular/compiler": "5.2.4",
    "@angular/core": "5.2.4",
    "@angular/forms": "5.2.4",
    "@angular/http": "5.2.4",
    "@angular/platform-browser": "5.2.4",
    "@angular/platform-browser-dynamic": "5.2.4",
    "@angular/router": "5.2.4",
    "@ngrx/effects": "^4.1.0",
    "@ngrx/router-store": "^4.1.0",
    "@ngrx/store": "^4.1.0",
    "@ngrx/store-devtools": "^4.1.1",
    "@nrwl/nx": "^0.6.19",
    "core-js": "^2.4.1",
    "ng-packagr": "^2.0.0",
    "rxjs": "^5.5.2",
    "three": "^0.88.0",
    "three.ar.js": "^0.1.6",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.29",
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "5.2.4",
    "@angular/language-service": "5.2.4",
    "@compodoc/compodoc": "^1.0.5",
    "@nrwl/schematics": "^0.6.19",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.54",
    "@types/three": "^0.84.34",
    "codelyzer": "~4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "2.6.2"
  }
}
